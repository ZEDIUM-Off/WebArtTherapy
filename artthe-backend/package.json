{
    "name": "artthe-backend",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "build:cli": "webpack",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test,client}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@fastify/static": "^5.0.0",
        "@nestjs/common": "^8.0.0",
        "@nestjs/core": "^8.0.0",
        "@nestjs/platform-express": "^8.4.6",
        "@nestjs/platform-fastify": "^8.4.5",
        "@nestjs/platform-socket.io": "^8.4.5",
        "@nestjs/websockets": "^8.4.5",
        "device-detector-js": "^3.0.3",
        "hbs": "^4.2.0",
        "local-ipv4-address": "^0.0.2",
        "peerjs": "^1.4.6",
        "qrcode": "^1.5.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.2.0",
        "socket.io-client": "^4.5.1",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@babylonjs/core": "^5.1.0",
        "@mediapipe/camera_utils": "^0.3.1640029074",
        "@mediapipe/control_utils": "^0.6.1629159505",
        "@mediapipe/drawing_utils": "^0.3.1620248257",
        "@mediapipe/hands": "^0.4.1646424915",
        "@nestjs/cli": "^8.0.0",
        "@nestjs/schematics": "^8.0.0",
        "@nestjs/testing": "^8.0.0",
        "@types/cannon": "^0.1.8",
        "@types/express": "^4.17.13",
        "@types/jest": "27.5.0",
        "@types/node": "^16.0.0",
        "@types/qrcode": "^1.4.2",
        "@types/supertest": "^2.0.11",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "babylonjs": "^5.1.0",
        "cannon": "github:schteppe/cannon.js",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "28.0.3",
        "prettier": "^2.3.2",
        "source-map-support": "^0.5.20",
        "supertest": "^6.1.3",
        "ts-jest": "28.0.1",
        "ts-loader": "^9.3.0",
        "ts-node": "^10.0.0",
        "tsconfig-paths": "4.0.0",
        "typescript": "^4.3.5",
        "webpack": "^5.72.1",
        "webpack-cli": "^4.9.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
